String e = "2.";
Double snippet;

int i = 0;
boolean done = false;

void setup(){
  while(!done){
    snippet = Double.parseDouble(e.substring(i, i + 10));
    if(isPrime(snippet)){
      done = true;
    }
    i++;
  }
  System.out.println(snippet);
}

public boolean isPrime(double num){
  if(num <= 1 || num % 1 != 0){
    return(false);
  }
  
  for(int i = 2; i <= (int)Math.sqrt(num); i++){
    if(num % i == 0){
      return(false);
    }
  }
  return(true);
}
